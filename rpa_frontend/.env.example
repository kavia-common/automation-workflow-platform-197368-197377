# Frontend environment configuration
# Toggle mock mode to use local mock data instead of live backend APIs
# When true, all API calls are routed to src/mocks adapters with simulated latency
REACT_APP_USE_MOCKS=true

# Optional backend base URL (used only when REACT_APP_USE_MOCKS is not "true")
REACT_APP_API_BASE_URL=http://localhost:3001

# Other available environment variables for this project (not required here)
# REACT_APP_API_BASE
# REACT_APP_BACKEND_URL
# REACT_APP_FRONTEND_URL
# REACT_APP_WS_URL
# REACT_APP_NODE_ENV
# REACT_APP_NEXT_TELEMETRY_DISABLED
# REACT_APP_ENABLE_SOURCE_MAPS
# REACT_APP_PORT
# REACT_APP_TRUST_PROXY
# REACT_APP_LOG_LEVEL
# REACT_APP_HEALTHCHECK_PATH
# REACT_APP_FEATURE_FLAGS
# REACT_APP_EXPERIMENTS_ENABLED
